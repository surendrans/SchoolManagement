<%= form_for @cast do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :cast_name %><br />
    <%= f.text_field :cast_name %>
  </p>
  <p>
    <%= f.label :cast_des %><br />
    <%= f.text_area :cast_des %>
  </p>
  <p>
    <%= f.label :cast_active %><br />
    <%= f.check_box :cast_active %>
  </p>
  <p><%= f.submit %></p>
<% end %>
