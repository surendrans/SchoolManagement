<% title "Emp Types" %>

<table>
  <tr>
    <th>Emptype Name</th>
    <th>Emptype Des</th>
    <th>Emptype Active</th>
  </tr>
  <% for emp_type in @emp_types %>
    <tr>
      <td><%= emp_type.emptype_name %></td>
      <td><%= emp_type.emptype_des %></td>
      <td><%= emp_type.emptype_active %></td>
      <td><%= link_to_show "Show", emp_type %></td>
      <td><%= link_to_edit "Edit", edit_emp_type_path(emp_type) %></td>
      <td><%= link_to_destroy "Destroy", emp_type, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to_new "New Emp Type", new_emp_type_path %></p>
